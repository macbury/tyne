<div id="tickets">
<% @tickets.each do |ticket| %>
  <div class="ticket" data-id="<%= ticket.id %>">
    <div class="title"><%= link_to ticket.title, ticket %></div>
    <div class="description"><%= ticket.description %></div>
  </div>
<% end %>
</div>

<br />

<%= link_to 'New Ticket', new_ticket_path, :class => "button" %>
